{
  "name": "deadlock-france-webapp-backend",
  "version": "1.0.0",
  "description": "Deadlock back",
  "main": "index.js",
  "directories": {
    "doc": "docs"
  },
  "scripts": {
    "start": "node dist/index.js",
    "dev": "nodemon --exec ts-node src/index.ts",
    "db-create": "psql -U deadlock -d deadlockdb -f data/create_tables.sql",
    "db-seed": "psql -U deadlock -d deadlockdb -f data/seeding_tables.sql",
    "db-reset": "npm run db-create && npm run db-seed",
    "build": "tsc"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Neyticom/Deadlock-France-webapp-backend.git"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/Neyticom/Deadlock-France-webapp-backend/issues"
  },
  "homepage": "https://github.com/Neyticom/Deadlock-France-webapp-backend#readme",
  "dependencies": {
    "dotenv": "^16.4.7",
    "express": "^4.21.2",
    "joi": "^17.13.3",
    "pg": "^8.13.1",
    "pg-hstore": "^2.3.4",
    "sequelize": "^6.37.5",
    "sequelize-typescript": "^2.1.6"
  },
  "devDependencies": {
    "@types/dotenv": "^8.2.3",
    "@types/express": "^5.0.0",
    "@types/node": "^22.10.2",
    "@types/sequelize": "^4.28.20",
    "dotenv-cli": "^8.0.0",
    "nodemon": "^3.1.7",
    "ts-node": "^10.9.2",
    "ts-node-dev": "^2.0.0",
    "typescript": "^5.7.2"
  }
}
